from bottle import run, route

@route('/')
def index():
    return '<a href="/about">About</a> <br>'\
           '<a href="/biography">Biography</a> <br>'\
           '<a href="/pictures">Pictures</a>'

@route('/about')
def about():
    return 'About'


@route('/biography')
def biography():
    return 'gjvv'


@route('/pictures')
def pictures():
    return 'gjvg'


run()
